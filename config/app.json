      "baseUrl": "https://openart.ai",
      "contestListUrl": "https://openart.ai/workflows",
      "selectors": {
        "contestItems": ".workflow-item, [class*=\"workflow\"], [class*=\"contest\"]",
        "title": "h1, h2, h3, h4, .title, [class*=\"title\"]",
        "description": ".description, .content, p",
        "deadline": ".deadline, .date, [class*=\"deadline\"], [class*=\"date\"]",
        "prize": ".prize, .reward, [class*=\"prize\"], [class*=\"reward\"]",
        "link": "a[href]"
      },
      "enabled": false,
      "delay": 1.5,
      "maxRetries": 3
