{
  "sources": [
    {
      "name": "modelscope",
      "displayName": "ModelScope",
      "baseUrl": "https://modelscope.cn",
      "contestListUrl": "https://modelscope.cn/competition",
      "selectors": {
        "contestItems": "a[href*=\"/competition/\"], [class*=\"competition\"], [class*=\"contest\"]",
        "title": "h3, h4, .title, [class*=\"title\"]",
        "description": ".description, .desc, p",
        "deadline": ".deadline, .time, [class*=\"deadline\"], [class*=\"time\"]",
        "prize": ".prize, .reward, [class*=\"prize\"], [class*=\"reward\"]",
        "link": "a[href]"
      },
      "enabled": true,
      "debugExtraction": true,
      "delay": 3.0,
      "maxRetries": 3
    },
    {
      "name": "civitai",
      "displayName": "Civitai",
      "baseUrl": "https://civitai.com",
      "contestListUrl": "https://civitai.com/events",
      "selectors": {
        "contestItems": ".AspectRatioImageCard_content__IGj_A, .AspectRatioImageCard_linkOrClick__d_K_4, [class*='AspectRatioImageCard']",
        "title": "[class*=\"AspectRatioImageCard_footer__FOU7a\"], h3, h4, .title",
        "description": ".AspectRatioImageCard_header__Mmd__, .description, .content, p",
        "deadline": ".mantine-Text-root, .date, [class*=\"date\"], .Cards_statChip__hnbES",
        "prize": ".Cards_statChip__hnbES, .prize, .reward",
        "link": "a, .AspectRatioImageCard_linkOrClick__d_K_4"
      },
      "enabled": true,
      "delay": 2.0,
      "maxRetries": 3
    },
    {
      "name": "openart",
      "displayName": "OpenArt",
      "baseUrl": "https://openart.ai",
      "contestListUrl": "https://openart.ai/workflows/challenges",
      "selectors": {
        "contestItems": "h6, .MuiTypography-h6, [class*='challenge']",
        "title": "h6, .MuiTypography-h6",
        "description": "p, .MuiTypography-body1",
        "deadline": ".date, [class*='date']",
        "prize": ".prize, .reward",
        "link": "a[href*='challenge']"
      },
      "enabled": false,
      "delay": 1.5,
      "maxRetries": 3
    },
    {
      "name": "devpost",
      "displayName": "Devpost",
      "baseUrl": "https://devpost.com",
      "contestListUrl": "https://devpost.com/hackathons",
      "selectors": {
        "contestItems": ".hackathon-tile",
        "title": ".main-content h3",
        "description": ".challenge-overview",
        "link": "a.tile-anchor",
        "deadline": ".time-left",
        "prize": ".prize-amount"
      },
      "enabled": true,
      "delay": 5.0,
      "maxRetries": 3
    }
  ],
  "aiProcessor": {
    "apiEndpoint": "https://models.github.ai/inference",
    "modelName": "ai21-labs/AI21-Jamba-1.5-Large",
    "maxTokens": 1000,
    "batchSize": 5
  },
  "storage": {
    "dataDir": "./data",
    "enableGitLFS": false,
    "maxFileSize": 50,
    "backupCount": 10
  },
  "site": {
    "title": "AI Contest Navigator",
    "description": "Discover the latest AI/ML competitions and challenges",
    "pageSize": 20,
    "enableBanner": true,
    "bannerContent": "ðŸš€ Welcome to AI Contest Navigator - Your gateway to AI competitions!"
  },
  "enableValidation": true,
  "schedule": {
    "crawlInterval": 8,
    "enableAutoRun": false
  },
  "notifications": {
    "enabled": false,
    "fromEmail": "AI Contest Navigator <onboarding@resend.dev>",
    "toEmails": []
  }
}
