{
  "name": "ai-contests-nav",
  "version": "1.0.0",
  "description": "AI contests navigator - Automated aggregation platform for AI competition information",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "node dist/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,vue,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,vue,json}\"",
    "crawl": "ts-node src/main.ts crawl",
    "process": "ts-node src/main.ts process",
    "status": "ts-node src/main.ts status",
    "export": "ts-node src/main.ts export",
    "cleanup": "ts-node src/main.ts cleanup",
    "run": "ts-node src/main.ts run",
    "aggregate:contests": "ts-node src/scripts/aggregate-contests.ts",
    "clean": "rimraf dist",
    "prepare": "husky install"
  },
  "keywords": [
    "ai",
    "contest",
    "competition",
    "aggregator",
    "web-scraping",
    "static-site-generator"
  ],
  "author": "AI Contest Team",
  "license": "MIT",
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.10"
  },
  "dependencies": {
    "@nuxtjs/color-mode": "^3.5.2",
    "autoprefixer": "^10.4.19",
    "axios": "^1.5.0",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.0.0",
    "fs-extra": "^11.1.1",
    "nuxt": "^3.13.0",
    "postcss": "^8.4.38",
    "puppeteer": "^21.1.1",
    "tailwindcss": "^3.4.10",
    "vue": "^3.5.20",
    "vue-router": "^4.5.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-contests/nav.git"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
