{
  "name": "ai-contests-nav",
  "version": "1.0.0",
  "description": "AI contests navigator - Automated aggregation platform for AI competition information",
  "main": "dist/index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "crawl": "ts-node --project tsconfig.cli.json src/main.ts crawl",
    "process": "ts-node --project tsconfig.cli.json src/main.ts process",
    "status": "ts-node --project tsconfig.cli.json src/main.ts status",
    "export": "ts-node --project tsconfig.cli.json src/main.ts export",
    "cleanup": "ts-node --project tsconfig.cli.json src/main.ts cleanup",
    "archive": "ts-node --project tsconfig.cli.json src/main.ts archive",
    "notify": "ts-node --project tsconfig.cli.json src/main.ts notify",
    "run": "ts-node --project tsconfig.cli.json src/main.ts run",
    "aggregate:contests": "ts-node --project tsconfig.cli.json src/scripts/aggregate-contests.ts",
    "clean": "rimraf dist .next",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "ai",
    "contest",
    "competition",
    "aggregator",
    "web-scraping",
    "nextjs",
    "react",
    "tailwindcss"
  ],
  "author": "nev4rb14su",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.49.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.30",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  },
