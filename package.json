    "@supabase/supabase-js": "^2.89.0",
    "@types/crypto-js": "^4.2.2",
    "@vercel/analytics": "^1.6.1",
    "axios": "^1.5.0",
    "cheerio": "^1.0.0-rc.12",
    "clsx": "^2.0.0",
    "commander": "^11.0.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.3",
    "framer-motion": "^10.16.4",
    "fs-extra": "^11.1.1",
    "https-proxy-agent": "^7.0.2",
    "lucide-react": "^0.292.0",
    "next": "^14.0.0",
    "puppeteer": "^22.0.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "resend": "^6.6.0",
    "tailwind-merge": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-contests/nav.git"
  },
